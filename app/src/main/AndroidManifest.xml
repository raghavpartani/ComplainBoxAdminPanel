<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tinderui">

    <application
        android:allowBackup="true"
        android:icon="@drawable/com"
        android:label="Complaint Box\nAdmin"
        android:roundIcon="@drawable/com"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Pending_Complaint"
            android:label="Pending Complaints">

        </activity>
        <activity
            android:name=".Pending_Requests"
            android:label="Pending Requests"
            android:logo="@drawable/com"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name=".SignUp"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>